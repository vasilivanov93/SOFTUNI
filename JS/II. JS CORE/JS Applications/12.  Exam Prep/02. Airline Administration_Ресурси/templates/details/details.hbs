<div class="ticket-area-left">
    <img src="{{img}}" alt="">
</div>
<div class="ticket-area-right">
    <h3>{{destination}}</h3>
    <div>from {{origin}}</div>
    <div class="data-and-time">
        {{departureDate}} {{departureTime}}
        {{#if isAuthor}}
            <a href="#/flights/editFlight/{{_id}}" class="edit-flight-detail"></a>
        {{/if}}
    </div>
    <div>
        {{seats}} Seats ({{cost}} per seat)
    </div>
</div>